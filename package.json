{
  "name": "@uniwebcms/site-content-collector",
  "version": "2.1.0",
  "description": "Collect and process website content from a folder structure",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./webpack": "./src/webpack/config.js",
    "./plugins": "./src/plugins/index.js"
  },
  "bin": {
    "collect-content": "./bin/collect-content.js"
  },
  "scripts": {
    "old-test": "jest",
    "old-test-report": "jest --json > test-results.json 2>&1",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test-report": "node --experimental-vm-modules node_modules/jest/bin/jest.js --json > test-results.json 2>&1"
  },
  "dependencies": {
    "@uniwebcms/content-reader": "^1.0.1",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0"
  },
  "peerDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.3",
    "webpack": "^5.97.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Proximify Inc.",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uniwebcms/site-content-collector.git"
  },
  "bugs": {
    "url": "https://github.com/uniwebcms/site-content-collector/issues"
  },
  "keywords": [
    "content",
    "site",
    "collector",
    "markdown",
    "yaml",
    "prosemirror",
    "cli"
  ]
}
