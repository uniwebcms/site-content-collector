{
  "name": "@uniwebcms/site-content-collector",
  "version": "2.2.6",
  "description": "Collect and process website content from a folder structure",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./host": "./src/hostWebpack/config.js",
    "./module": "./src/moduleWebpack/config.js",
    "./plugins": "./src/plugins/index.js"
  },
  "bin": {
    "collect-content": "./bin/collect-content.js"
  },
  "scripts": {
    "old-test": "jest",
    "old-test-report": "jest --json > test-results.json 2>&1",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test-report": "node --experimental-vm-modules node_modules/jest/bin/jest.js --json > test-results.json 2>&1"
  },
  "dependencies": {
    "@uniwebcms/content-reader": "^1.0.3",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0",
    "autoprefixer": "^10.4.13",
    "compression-webpack-plugin": "^10.0.0",
    "image-size": "^1.1.1",
    "postcss-preset-env": "^7.8.3",
    "sharp": "^0.33.5",
    "uuid": "^9.0.0",
    "sass": "^1.50.0",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
